@model IEnumerable<Webinar.Database.Webinar>

@{
    Layout = null;
}



<head>

    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <script src="~/Content/jquery-3.3.1.js"></script>
    <script src="~/Content/dataTables.bootstrap4.min.js"></script>
    <script src="~/Content/jquery.dataTables.js"></script>
   


</head>


<div class="container">
    <div class="row">
       
        <h2 style="text-align:center"><img src="~/WebinarStyle/images/Logo-1.png" /></h2>
    </div>
    <h1 style="text-align:center">Dynamic DNA Webinar  </h1>
    <table id="example" class="table table-striped table-bordered" style="width:100%">

        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Company</th>
                <th>Message</th>
                <th>Day</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {




                <tr>
                    <td>@Html.DisplayFor(modelItem => item.Name)</td>


                    <td>@Html.DisplayFor(modelItem => item.Email)</td>


                    <td>@Html.DisplayFor(modelItem => item.Company)</td>


                    <td>@Html.DisplayFor(modelItem => item.Message)</td>

                    <td>@Html.DisplayFor(modelItem => item.Day)</td>


                </tr>
            }
        </tbody>

    </table>

</div>





<script>
    $(document).ready(function () {
        $('#example').DataTable();
    });
</script>